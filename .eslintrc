{
  "rules": {
    "semi": 2,
    "comma-dangle": 2,
    "arrow-parens": 2,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "next"
      }
    ],
    "no-use-before-define": [
      "error",
      {
        "variables": false
      }
    ],
    "no-multi-str": 0
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    "babel"
  ]
}